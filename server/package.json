{
  "name": "demo-server",
  "version": "1.0.0",
  "description": "Node.js Express MongoDB server",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon",
    "dev:http": "PORT=3002 nodemon",
    "dev:https": "PORT=3443 nodemon",
    "dev:watch": "nodemon --watch . --ext js,json --ignore node_modules/",
    "dev:force": "nodemon --watch . --ext js,json --ignore node_modules/ --delay 3s",
    "clean": "taskkill /IM node.exe /F",
    "start-clean": "npm run clean && timeout /t 2 /nobreak && npm start",
    "restart": "npm run clean && npm run dev"
  },
  "keywords": [
    "nodejs",
    "express",
    "mongodb"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.12.2",
    "bcrypt": "^6.0.0",
    "body-parser": "^1.20.2",
    "cloudinary": "^2.7.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.2.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "selfsigned": "^3.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.1.10"
  }
}
